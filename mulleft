#include<iostream>
using namespace std;
int main()
{
 int a[50],n,sum1=0,sum2=0,ans;
  cin>>n;
  for(int i=0;i<n;i++)
   {
    cin>>a[i];
    }
    ans=n/2;
    for(int i=0;i<ans;i++)
     {
      sum1=sum1+i;
      }
     for(int i=ans;i<n;i++)
      {
        sum2=sum2+i;
        }
      cout<<sum1*sum2;
return 0;
}
